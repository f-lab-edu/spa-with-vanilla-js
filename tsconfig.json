{
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": "./",
    "allowJs": true,
    "target": "ES6",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "lib": ["ES2015", "DOM", "DOM.Iterable"],
  },
  "exclude": ["node_modules",]

}

